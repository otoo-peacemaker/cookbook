<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bottom_sheet_nav"
    app:startDestination="@id/startRide">

    <fragment
        android:id="@+id/startRide"
        android:name="com.example.googlemap.StartRide"
        android:label="fragment_start_ride"
        tools:layout="@layout/fragment_start_ride" >
        <action
            android:id="@+id/action_startRide_to_startRideDetails"
            app:destination="@id/startRideDetails" />
    </fragment>
    <fragment
        android:id="@+id/startRideDetails"
        android:name="com.example.googlemap.StartRideDetails"
        tools:layout="@layout/fragment_start_ride_details"
        android:label="StartRideDetails" />
    <activity
        android:id="@+id/mapsActivity"
        android:name="com.example.googlemap.MapsActivity"
        android:label="MapsActivity" />
</navigation>